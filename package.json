{
  "name": "ace-diff",
  "version": "3.2.0",
  "description": "A diff/merging wrapper for Ace Editor built on google-diff-match-patch",
  "source": "src/index.js",
  "main": "dist/ace-diff.min.js",
  "module": "dist/module.js",
  "scripts": {
    "build": "rm -rf .parcel-cache && parcel build && sass src/styles/ace-diff-twilight.scss dist/ace-diff-twilight.min.css --style=compressed",
    "watch": "parcel watch",
    "dev": "parcel serve test/fixtures/index.html --open",
    "serve": "parcel test/fixtures/*.html -p 8081",
    "lint": "eslint src",
    "test": "cypress run",
    "cypress": "cypress open",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ace-diff/ace-diff.git"
  },
  "keywords": [
    "ace",
    "dif",
    "diffing",
    "merge",
    "merging",
    "ace editor",
    "syntax highlighting"
  ],
  "author": "Ben Keen, Jason Thomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ace-diff/ace-diff/issues"
  },
  "browserslist": "> 0.2%, chrome >= 80, edge >= 80, firefox ESR, safari >= 13.1, not dead, not ie 11, not op_mini all",
  "homepage": "https://ace-diff.github.io/ace-diff/",
  "dependencies": {
    "@sanity/diff-match-patch": "3.2.0"
  },
  "devDependencies": {
    "@parcel/babel-preset-env": "^2.16.3",
    "@parcel/transformer-sass": "^2.16.3",
    "cypress": "^15.8.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "parcel": "^2.16.3",
    "prettier": "3.7.4",
    "sass": "1.97.2"
  }
}
