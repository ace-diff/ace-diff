  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.5.3",
    "sass": "1.87.0",
